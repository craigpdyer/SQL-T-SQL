>>
>> create table dup
>> (
>> id int iDentity(1,1),
>> name varchar(30)
>> )
>> insert into dup values('Superman')
>>
>> select * from (select *, row_number() Over(partition by name order by
>> name ) as rnk from dup) Dup
>> where rnk>1
>>
