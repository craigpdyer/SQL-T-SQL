> Alter  proc pale_sp(@word varchar(300))
> as
> begin
> Declare @wordcpy varchar(300)=@word
> Declare @revword varchar(300)=''
>
>
>     while (0 < len(@word))
>         begin
>         set @revword = @revword + right(@word,1)
>         set @word = substring( @word,1,len(@word)-1)
>
>         end
>
>     if @wordcpy = @revword
>         print 'is a paladrom'
>     else
>         print 'is not paladrom'
>
>
> end
>
